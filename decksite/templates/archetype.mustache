<h2>{{name}}</h2>
<section>
    <h3>Archetype Tree</h3>
    {{> archetypetree}}
</section>
<section>
    <h3>Most Common Cards</h3>
    <ul>
        {{#most_common_cards}}<li>{{> singlecard}}</li>{{/most_common_cards}}
    </ul>
</section>
<section>
    <h3>Top Decks</h3>
    {{#best_decks}}{{> decktable}}{{/best_decks}}
</section>
<section>
    <h3>Matchups</h3>
    {{#matchups_container}}
        {{> archetypetree}}
    {{/matchups_container}}
</section>
<section>
    <h3>All Decks</h3>
    {{> decktable}}
</section>
